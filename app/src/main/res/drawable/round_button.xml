<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="oval">
            <stroke android:color="#4DFFE133"
                android:width="2dp"/>
            <solid android:color="#26372420"/>
            <size
                android:width="120dp"
                android:height="120dp"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <stroke android:color="#FFFFE133"
                android:width="3dp"/>
            <solid android:color="#FFE166"/>
            <size
                android:width="120dp"
                android:height="120dp"/>
        </shape>
    </item>
</selector>